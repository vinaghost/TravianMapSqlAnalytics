@model List<int>

@{
    var name = ViewData["Name"] as string;

    var htmlAttributes = new Dictionary<string, object>
    {
        {"class", $"form-select {Constants.Select2Combox}"},
        {"data-ajax--url", $"{ViewData["Url"]}"},
    };
}

<div class="input-group mt-3 list-parameter">
    <span class="input-group-text">
        @Html.Label(name, name, new { @class = "control-label", style = "width: 100%", })
    </span>

    @Html.ListBox(name, [], htmlAttributes)
</div>

